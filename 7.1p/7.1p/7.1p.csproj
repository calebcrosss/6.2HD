<Project Sdk="Microsoft.NET.Sdk" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>_7._1p</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="**/*.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\app.config" Link="app.config" />
  </ItemGroup>

  <Target Name="Build">
    <PropertyGroup>
      <OutputPath>$(OutputPath)\bin\$(Configuration)</OutputPath>
    </PropertyGroup>
    <ItemGroup>
      <Compile Include="**/*.cs" />
    </ItemGroup>
    <Exec Command="C:\Path\To\Compiler.exe $(Compile)" />  </Target>

</Project>

